import random
num1=random.uniform(1,100)
print("1st no.",round(num1,2))
num2=random.uniform(1,100)
print("2nd no.",round(num2,2))
num3=random.uniform(1,100)
print("3rd no.",round(num3,2))
num4=random.uniform(1,100)
print("4th no.",round(num4,2))
num5=random.uniform(1,100)
print("5th no.",round(num5,2))
ave=(num1+num2+num3+num4+num5)/5
print("The total average is:",round(ave,2))
